def writeToFile (text = '') {
	touch $text
	'date > $text'
	cat $text
	rm $text
}

pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
    stages {
    	stage('One'){
		steps {
			writeToFile('file.txt')
			sleep '10s'
			writeToFile('file.txt')
		}
	}
    }
}
