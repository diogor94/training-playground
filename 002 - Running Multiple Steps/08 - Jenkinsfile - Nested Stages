pipeline {
    agent none
     stages {
	 	stage ("nested") {
			agent { label "nested" }
			stages {
				stage ("Stage 1") {
					agent { docker 'maven:latest' }
					steps {
						sh "mvn --version"
					}
				}
				stage ("Stage 2") {
					agent { docker 'node:latest' }
					steps {
						sh "npm --version"
					}
				}
			}
		}
    }
}
